text(band=header alignment="0" text="7. 제16조 제1항에 의한 공사의 정지기간이 전체공사 기간의 50/100 이상인 때" border="0" color="0" x="40" y="856" height="14" width="665" html.valueishtml="0"  name=t_37 visible="1"  font.face="바탕체" font.height="-9" font.weight="400"  font.family="1" font.pitch="1" font.charset="129" background.mode="1" background.color="536870912" )
