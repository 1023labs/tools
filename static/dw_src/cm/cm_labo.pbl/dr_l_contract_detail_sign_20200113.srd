 retrieve="SELECT A.COMPANY_CODE, 
        A.LABOR_CODE, 
        B.LABOR_NAME, 
        X_DECRYPT(B.RRN_PRE, '(enckey)') || X_DECRYPT(B.RRN_POST, '(enckey)') AS LABOR_ID,
                 X_DECRYPT(B.ADDR1, '(enckey)') || X_DECRYPT(B.ADDR2, '(enckey)')      AS ADDRESS,
                 X_DECRYPT(B.PHONE, '(enckey)') AS PHONE,
                 X_DECRYPT(B.HP, '(enckey)')    AS HP,
        A.PROJ_CODE, 
        C.PROJ_NAME, 
        A.REG_DATE, 
        C.HEAD_EMPNO AS HEAD_EMPNO, 
        DECODE(:as_comp_code, '100', '(주)세보엠이씨', '(주)에스비테크') AS ASSOCIATION_NAME, 
        DECODE(:as_comp_code,'100', '김우영','김재영') AS CEO_NAME, 
        DECODE(:as_comp_code,'100', '김진호','') AS CEO_NAME2, 
        C.ADDR1 ||' '||C.ADDR2 AS PROJ_ADDRESS, 
        D.BANK_CODE AS BANK_CODE, 
        F.BANK_NAME, 
        D.BANK_ACCOUNT AS BANK_ACCOUNT, 
        A.PAY_COST, 
        A.START_DATE, 
        A.END_DATE, 
        A.JOB_CODE, 
        E.DNAME AS JOB_NAME, 
        (A.PAY_COST * G.BASIC_TAX * 0.01 / 8) AS TIME_AMT, 
         A.PAY_COST * G.BASIC_TAX * 0.01 AS BASIC_AMT,
         A.PAY_COST * G.JU_TAX * 0.01 AS JU_AMT,
         A.PAY_COST * G.TIME_ETC_TAX * 0.01 AS TIME_ETC_AMT,
         A.PAY_COST * G.HOLIDAY_CUT_TAX * 0.01 AS HOLIDAY_CUT_AMT,
         A.PAY_COST * G.YEAR_TAX * 0.01 AS YEAR_AMT,
         G.BASIC_TAX,
         G.JU_TAX,
         G.TIME_ETC_TAX,
         G.HOLIDAY_CUT_TAX,
         G.YEAR_TAX,
         G.TOTAL_TAX,
         H.EMP_NAME,
         G.OVER_TAX,
         A.PAY_COST * G.OVER_TAX * 0.01 AS OVER_AMT,
        TO_CHAR(A.SIGN_DATE,'YYYYMMDD') AS SIGN_DATE,
        A.SIGN_FILEPATH as SIGN_FILEPATH,
        A.SIGN_FILENAME as SIGN_FILENAME,
        NVL(I.START_TIME_A,'') AS START_TIME_A,
        NVL(I.END_TIME_A,'') AS END_TIME_A,
        NVL(I.START_TIME_B,'') AS START_TIME_B,
        NVL(I.END_TIME_B,'') AS END_TIME_B,
        NVL(I.START_TIME_C,'') AS START_TIME_C,
        NVL(I.END_TIME_C,'') AS END_TIME_C,
        NVL(I.START_TIME_D,'') AS START_TIME_D,
        NVL(I.END_TIME_D,'') AS END_TIME_D,
        NVL(I.START_TIME_E,'') AS START_TIME_E,
        NVL(I.END_TIME_E,'') AS END_TIME_E,
        NVL(I.START_TIME_F,'') AS START_TIME_F,
        NVL(I.END_TIME_F,'') AS END_TIME_F,
        NVL(I.TIME_A_H,'') AS TIME_A_H,
        NVL(I.TIME_B_H,'') AS TIME_B_H,
        NVL(I.TIME_C_H,'') AS TIME_C_H,
        NVL(I.TIME_D_H,'') AS TIME_D_H,
        NVL(I.TIME_E_H,'') AS TIME_E_H,
        NVL(I.TIME_F_H,'') AS TIME_F_H,
        NVL(I.TEXT_AREA,'') AS TEXT_AREA
FROM CM_LABO_CONTRACT A INNER JOIN CM_LABO_MASTER B 
                               ON A.COMPANY_CODE = B.COMPANY_CODE AND A.LABOR_CODE = B.LABOR_CODE 
                        LEFT JOIN SM_CODE_PROJECT C 
                               ON A.PROJ_CODE = C.PROJ_CODE 
                        LEFT JOIN CM_LABO_ACCOUNT D 
                               ON A.COMPANY_CODE = D.COMPANY_CODE AND A.LABOR_CODE = D.LABOR_CODE AND D.USE_YN = 'Y' 
                        LEFT JOIN SM_COMCODE_D E 
                               ON A.JOB_CODE = E.DCODE AND E.SYS_ID = 'CM' AND E.HCODE = 'JOBDIV' AND E.USE_YN = 'Y' 
                        LEFT JOIN HR_CODE_BANK F 
                               ON D.BANK_CODE = F.BANK_CODE 
                        LEFT JOIN CM_LABO_CONTRACT_TAX G 
                               ON A.COMPANY_CODE = G.COMPANY_CODE 
                        LEFT JOIN HR_PERS_MASTER H 
                               ON C.HEAD_EMPNO = H.EMP_NO 
                        LEFT JOIN CM_PROJ_SET_CONT_TIME I 
                               ON A.COMPANY_CODE = I.COMPANY_CODE
                              AND A.PROJ_CODE    = I.PROJ_CODE 
WHERE A.COMPANY_CODE = :as_comp_code 
    AND A.LABOR_CODE = :as_labor_code 
    AND A.REG_DATE = :as_reg_date 
    AND A.CONTRACT_NO = :as_cont_no
     AND G.YYYYMM = (SELECT MAX(YYYYMM) FROM CM_LABO_CONTRACT_TAX
                     WHERE YYYYMM <= SUBSTR(A.START_DATE,1,6))
ORDER BY A.REG_DATE " arguments=(("as_comp_code", string),("as_labor_code", string),("as_reg_date", string),("as_cont_no", string)) )
