 retrieve="  SELECT A.COMPANY_CODE, 
         A.LABOR_CODE, 
         B.LABOR_NAME,
         B.RRN_PRE || B.RRN_POST AS LABOR_ID,
         B.ADDR1 || B.ADDR2      AS ADDRESS,
         B.PHONE AS PHONE,
         B.HP    AS HP,
         A.PROJ_CODE,
         C.PROJ_NAME,
         C.HEAD_EMPNO AS HEAD_EMPNO, 
         DECODE(:as_comp_code, '100', '(주)세보엠이씨', '(주)에스비테크') AS ASSOCIATION_NAME,
         C.ADDR1 AS PROJ_ADDRESS,         
         D.BANK_CODE    AS BANK_CODE,
         F.BANK_NAME, 
         D.BANK_ACCOUNT AS BANK_ACCOUNT, 
         A.PAY_COST,
         A.JOB_CODE,
         E.DNAME AS JOB_NAME
    FROM CM_LABO_CONTRACT A INNER JOIN CM_LABO_MASTER B
                                    ON A.COMPANY_CODE = B.COMPANY_CODE AND A.LABOR_CODE = B.LABOR_CODE    
                             LEFT JOIN SM_CODE_PROJECT C
                                    ON A.PROJ_CODE = C.PROJ_CODE
                             LEFT JOIN CM_LABO_ACCOUNT D
                                    ON A.COMPANY_CODE = D.COMPANY_CODE AND A.LABOR_CODE = D.LABOR_CODE AND D.USE_YN = 'Y'    
                             LEFT JOIN SM_COMCODE_D E
                                    ON A.JOB_CODE = E.DCODE AND E.SYS_ID = 'CM' AND E.HCODE = 'JOBDIV' AND E.USE_YN = 'Y' 
                             LEFT JOIN HR_CODE_BANK F
                                    ON D.BANK_CODE = F.BANK_CODE                                                                         
   WHERE A.COMPANY_CODE = :as_comp_code
     AND A.LABOR_CODE   = :as_labor_code
ORDER BY A.REG_DATE   " arguments=(("as_comp_code", string),("as_labor_code", string)) )
