       DECODE(A.PLAN09,0,0,TRUNC(100 - TRUNC((B.PLAN09/A.PLAN09)*100,2),2))            AS PLAN09,
       DECODE(A.PLAN10,0,0,TRUNC(100 - TRUNC((B.PLAN10/A.PLAN10)*100,2),2))             AS PLAN10,
       DECODE(A.PLAN11,0,0,TRUNC(100 - TRUNC((B.PLAN11/A.PLAN11)*100,2),2))             AS PLAN11,
       DECODE(A.PLAN12,0,0,TRUNC(100 - TRUNC((B.PLAN12/A.PLAN12)*100,2),2))             AS PLAN12,
       DECODE(A.YEAR01,0,0,TRUNC(100 - TRUNC((B.YEAR01/A.YEAR01)*100,2),2))             AS YEAR01,
       DECODE(A.YEAR02,0,0,TRUNC(100 - TRUNC((B.YEAR02/A.YEAR02)*100,2),2))             AS YEAR02,
       '99'                            AS SORT_ORDER,
       '5'                             AS SORT_ORDER2,
       DECODE(A.PLAN_TOT_AMT,0,0,TRUNC(100 - TRUNC((B.PLAN_TOT_AMT/A.PLAN_TOT_AMT)*100,2),2)) AS PLAN_TOT_AMT,
       DECODE(A.SUM_TOT_AMT,0,0,TRUNC(100 - TRUNC((B.SUM_TOT_AMT/A.SUM_TOT_AMT)*100,2),2))    AS SUM_TOT_AMT,
       DECODE(A.ETC_TOT_AMT,0,0,TRUNC(100 - TRUNC((B.ETC_TOT_AMT/A.ETC_TOT_AMT)*100,2),2))    AS ETC_TOT_AMT
  FROM CM_EXEC_PLAN_MONTH_TEMP A LEFT JOIN (SELECT A.COMPANY_CODE     AS COMPANY_CODE,
                                                   A.PROJ_CODE        AS PROJ_CODE,
                                                   A.CONST_NAME       AS CONST_NAME,
                                                   '소   계'             AS CONST_ACNT_NAME,
                                                   SUM(A.EXEC_AMT)         AS EXEC_AMT,
                                                   SUM(A.CHG_AMT)          AS CHG_AMT,
                                                   SUM(A.PRE_YEAR_AMT)     AS PRE_YEAR_AMT,
                                                   SUM(A.CUR_YEAR_AMT)     AS CUR_YEAR_AMT,
                                                   SUM(A.TOT_YEAR_AMT)     AS TOT_YEAR_AMT,
                                                   SUM(A.PLAN01)           AS PLAN01,
                                                   SUM(A.PLAN02)           AS PLAN02,
                                                   SUM(A.PLAN03)           AS PLAN03,
                                                   SUM(A.PLAN04)           AS PLAN04,
                                                   SUM(A.PLAN05)           AS PLAN05,
                                                   SUM(A.PLAN06)           AS PLAN06,
                                                   SUM(A.PLAN07)           AS PLAN07,
                                                   SUM(A.PLAN08)           AS PLAN08,
                                                   SUM(A.PLAN09)           AS PLAN09,
                                                   SUM(A.PLAN10)           AS PLAN10,
                                                   SUM(A.PLAN11)           AS PLAN11,
                                                   SUM(A.PLAN12)           AS PLAN12,
                                                   SUM(A.YEAR01)           AS YEAR01,
                                                   SUM(A.YEAR02)           AS YEAR02,
                                                   '99'                   AS SORT_ORDER,
                                                   '3'                    AS SORT_ORDER2,
                                                   SUM(A.PLAN_TOT_AMT)    AS PLAN_TOT_AMT,
                                                   SUM(A.SUM_TOT_AMT)     AS SUM_TOT_AMT,
                                                   SUM(A.ETC_TOT_AMT)      AS ETC_TOT_AMT
                                              FROM CM_EXEC_PLAN_MONTH_TEMP A
                                             WHERE A.COMPANY_CODE = :as_comp
                                               AND A.PROJ_CODE    = :as_proj
                                               AND A.SORT_ORDER2 = '2'  
                                            GROUP BY A.COMPANY_CODE,
                                                     A.PROJ_CODE,
                                                     A.CONST_NAME ) B
                                         ON A.COMPANY_CODE = B.COMPANY_CODE
                                        AND A.PROJ_CODE    = B.PROJ_CODE
 WHERE A.COMPANY_CODE = :as_comp
   AND A.PROJ_CODE    = :as_proj
   AND A.SORT_ORDER2 = '1'       
