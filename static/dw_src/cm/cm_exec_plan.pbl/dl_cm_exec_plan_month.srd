SELECT A.COMPANY_CODE                  AS COMPANY_CODE,
       A.PROJ_CODE                     AS PROJ_CODE,
       '공사이익'                      AS CONST_NAME,
       ''                              AS CONST_ACNT_NAME,
       A.EXEC_AMT - B.EXEC_AMT         AS EXEC_AMT,
       A.CHG_AMT - B.CHG_AMT           AS CHG_AMT,
       A.PRE_YEAR_AMT - B.PRE_YEAR_AMT AS PRE_YEAR_AMT,
       A.CUR_YEAR_AMT - B.CUR_YEAR_AMT AS CUR_YEAR_AMT,
       A.TOT_YEAR_AMT - B.TOT_YEAR_AMT AS TOT_YEAR_AMT,
       A.PLAN01 - B.PLAN01             AS PLAN01,
       A.PLAN02 - B.PLAN02             AS PLAN02,
       A.PLAN03 - B.PLAN03             AS PLAN03,
       A.PLAN04 - B.PLAN04             AS PLAN04,
       A.PLAN05 - B.PLAN05             AS PLAN05,
       A.PLAN06 - B.PLAN06             AS PLAN06,
       A.PLAN07 - B.PLAN07             AS PLAN07,
       A.PLAN08 - B.PLAN08             AS PLAN08,
       A.PLAN09 - B.PLAN09             AS PLAN09,
       A.PLAN10 - B.PLAN10             AS PLAN10,
       A.PLAN11 - B.PLAN11             AS PLAN11,
       A.PLAN12 - B.PLAN12             AS PLAN12,
       A.YEAR01 - B.YEAR01             AS YEAR01,
       A.YEAR02 - B.YEAR02             AS YEAR02,
       '99'                            AS SORT_ORDER,
       '4'                             AS SORT_ORDER2,
       A.PLAN_TOT_AMT - B.PLAN_TOT_AMT     AS PLAN_TOT_AMT,
       A.SUM_TOT_AMT - B.SUM_TOT_AMT     AS SUM_TOT_AMT,
       A.ETC_TOT_AMT - B.ETC_TOT_AMT      AS ETC_TOT_AMT
  FROM CM_EXEC_PLAN_MONTH_TEMP A LEFT JOIN (SELECT A.COMPANY_CODE     AS COMPANY_CODE,
                                                   A.PROJ_CODE        AS PROJ_CODE,
                                                   A.CONST_NAME       AS CONST_NAME,
                                                   '소계'             AS CONST_ACNT_NAME,
                                                   SUM(A.EXEC_AMT)         AS EXEC_AMT,
                                                   SUM(A.CHG_AMT)          AS CHG_AMT,
                                                   SUM(A.PRE_YEAR_AMT)     AS PRE_YEAR_AMT,
                                                   SUM(A.CUR_YEAR_AMT)     AS CUR_YEAR_AMT,
                                                   SUM(A.TOT_YEAR_AMT)     AS TOT_YEAR_AMT,
                                                   SUM(A.PLAN01)           AS PLAN01,
                                                   SUM(A.PLAN02)           AS PLAN02,
                                                   SUM(A.PLAN03)           AS PLAN03,
                                                   SUM(A.PLAN04)           AS PLAN04,
                                                   SUM(A.PLAN05)           AS PLAN05,
                                                   SUM(A.PLAN06)           AS PLAN06,
                                                   SUM(A.PLAN07)           AS PLAN07,
                                                   SUM(A.PLAN08)           AS PLAN08,
                                                   SUM(A.PLAN09)           AS PLAN09,
                                                   SUM(A.PLAN10)           AS PLAN10,
                                                   SUM(A.PLAN11)           AS PLAN11,
                                                   SUM(A.PLAN12)           AS PLAN12,
                                                   SUM(A.YEAR01)           AS YEAR01,
                                                   SUM(A.YEAR02)           AS YEAR02,
                                                   '99'                   AS SORT_ORDER,
                                                   '3'                    AS SORT_ORDER2,
                                                   SUM(A.PLAN_TOT_AMT)    AS PLAN_TOT_AMT,
                                                   SUM(A.SUM_TOT_AMT)     AS SUM_TOT_AMT,
                                                   SUM(A.ETC_TOT_AMT)      AS ETC_TOT_AMT
                                              FROM CM_EXEC_PLAN_MONTH_TEMP A
                                             WHERE A.COMPANY_CODE = :as_comp
                                               AND A.PROJ_CODE    = :as_proj
                                               AND A.SORT_ORDER2 = '2'  
                                            GROUP BY A.COMPANY_CODE,
                                                     A.PROJ_CODE,
                                                     A.CONST_NAME ) B
                                         ON A.COMPANY_CODE = B.COMPANY_CODE
                                        AND A.PROJ_CODE    = B.PROJ_CODE
 WHERE A.COMPANY_CODE = :as_comp
   AND A.PROJ_CODE    = :as_proj
   AND A.SORT_ORDER2 = '1'    
UNION ALL
SELECT A.COMPANY_CODE                                     AS COMPANY_CODE,
       A.PROJ_CODE                                        AS PROJ_CODE,
       '손익율'                                           AS CONST_NAME,
       ''                                                 AS CONST_ACNT_NAME,
       DECODE(A.EXEC_AMT,0,0,TRUNC(100 - TRUNC((B.EXEC_AMT/A.EXEC_AMT)*100,2),2))         AS EXEC_AMT,
       DECODE(A.CHG_AMT,0,0,TRUNC(100 - TRUNC((B.CHG_AMT/A.CHG_AMT)*100,2),2))           AS CHG_AMT,
       DECODE(A.PRE_YEAR_AMT,0,0,TRUNC(100 - TRUNC((B.PRE_YEAR_AMT/A.PRE_YEAR_AMT)*100,2),2)) AS PRE_YEAR_AMT,
       DECODE(A.CUR_YEAR_AMT,0,0,TRUNC(100 - TRUNC((B.CUR_YEAR_AMT/A.CUR_YEAR_AMT)*100,2),2)) AS CUR_YEAR_AMT,
       DECODE(A.TOT_YEAR_AMT,0,0,TRUNC(100 - TRUNC((B.TOT_YEAR_AMT/A.TOT_YEAR_AMT)*100,2),2)) AS TOT_YEAR_AMT,
       DECODE(A.PLAN01,0,0,TRUNC(100 - TRUNC((B.PLAN01/A.PLAN01)*100,2),2))             AS PLAN01,
       DECODE(A.PLAN02,0,0,TRUNC(100 - TRUNC((B.PLAN02/A.PLAN02)*100,2),2))             AS PLAN02,
       DECODE(A.PLAN03,0,0,TRUNC(100 - TRUNC((B.PLAN03/A.PLAN03)*100,2),2))             AS PLAN03,
       DECODE(A.PLAN04,0,0,TRUNC(100 - TRUNC((B.PLAN04/A.PLAN04)*100,2),2))             AS PLAN04,
       DECODE(A.PLAN05,0,0,TRUNC(100 - TRUNC((B.PLAN05/A.PLAN05)*100,2),2))             AS PLAN05,
       DECODE(A.PLAN06,0,0,TRUNC(100 - TRUNC((B.PLAN06/A.PLAN06)*100,2),2))            AS PLAN06,
       DECODE(A.PLAN07,0,0,TRUNC(100 - TRUNC((B.PLAN07/A.PLAN07)*100,2),2))             AS PLAN07,
