 retrieve="SELECT A.COMPANY_CODE     AS COMPANY_CODE,
       A.PROJ_CODE        AS PROJ_CODE,
       A.CONST_NAME       AS CONST_NAME,
       DECODE(A.CONST_ACNT_NAME,'공사매출','',A.CONST_ACNT_NAME)  AS CONST_ACNT_NAME,
       A.EXEC_AMT         AS EXEC_AMT,
       A.CHG_AMT          AS CHG_AMT,
       A.PRE_YEAR_AMT     AS PRE_YEAR_AMT,
       A.CUR_YEAR_AMT     AS CUR_YEAR_AMT,
       A.TOT_YEAR_AMT     AS TOT_YEAR_AMT,
       A.PLAN01           AS PLAN01,
       A.PLAN02           AS PLAN02,
       A.PLAN03           AS PLAN03,
       A.PLAN04           AS PLAN04,
       A.PLAN05           AS PLAN05,
       A.PLAN06           AS PLAN06,
       A.PLAN07           AS PLAN07,
       A.PLAN08           AS PLAN08,
       A.PLAN09           AS PLAN09,
       A.PLAN10           AS PLAN10,
       A.PLAN11           AS PLAN11,
       A.PLAN12           AS PLAN12,
       A.YEAR01           AS YEAR01,
       A.YEAR02           AS YEAR02,
       A.SORT_ORDER       AS SORT_ORDER,
       A.SORT_ORDER2      AS SORT_ORDER2,
       A.PLAN_TOT_AMT     AS PLAN_TOT_AMT,
       A.SUM_TOT_AMT      AS SUM_TOT_AMT,
       A.ETC_TOT_AMT      AS ETC_TOT_AMT
  FROM CM_EXEC_PLAN_MONTH_TEMP A
 WHERE A.COMPANY_CODE = :as_comp
   AND A.PROJ_CODE    = :as_proj
   AND A.CONST_ACNT_NAME NOT IN('공통자재공사','기계실배관공사')
UNION ALL
SELECT A.COMPANY_CODE     AS COMPANY_CODE,
       A.PROJ_CODE        AS PROJ_CODE,
       A.CONST_NAME       AS CONST_NAME,
       '소계'             AS CONST_ACNT_NAME,
       SUM(A.EXEC_AMT)         AS EXEC_AMT,
       SUM(A.CHG_AMT)          AS CHG_AMT,
       SUM(A.PRE_YEAR_AMT)     AS PRE_YEAR_AMT,
       SUM(A.CUR_YEAR_AMT)     AS CUR_YEAR_AMT,
       SUM(A.TOT_YEAR_AMT)     AS TOT_YEAR_AMT,
       SUM(A.PLAN01)           AS PLAN01,
       SUM(A.PLAN02)           AS PLAN02,
       SUM(A.PLAN03)           AS PLAN03,
       SUM(A.PLAN04)           AS PLAN04,
       SUM(A.PLAN05)           AS PLAN05,
       SUM(A.PLAN06)           AS PLAN06,
       SUM(A.PLAN07)           AS PLAN07,
       SUM(A.PLAN08)           AS PLAN08,
       SUM(A.PLAN09)           AS PLAN09,
       SUM(A.PLAN10)           AS PLAN10,
       SUM(A.PLAN11)           AS PLAN11,
       SUM(A.PLAN12)           AS PLAN12,
       SUM(A.YEAR01)           AS YEAR01,
       SUM(A.YEAR02)           AS YEAR02,
       '99'                   AS SORT_ORDER,
       '3'                    AS SORT_ORDER2,
       SUM(A.PLAN_TOT_AMT)     AS PLAN_TOT_AMT,
       SUM(A.SUM_TOT_AMT)      AS SUM_TOT_AMT,
       SUM(A.ETC_TOT_AMT)      AS ETC_TOT_AMT
  FROM CM_EXEC_PLAN_MONTH_TEMP A
 WHERE A.COMPANY_CODE = :as_comp
   AND A.PROJ_CODE    = :as_proj
   AND A.SORT_ORDER2 = '2'  
GROUP BY A.COMPANY_CODE,
         A.PROJ_CODE,
         A.CONST_NAME 
UNION ALL
