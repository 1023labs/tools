SELECT A.COMPANY_CODE                                     AS COMPANY_CODE,
       A.PROJ_CODE                                        AS PROJ_CODE,
       '원가율'                                           AS CONST_NAME,
       ''                                                 AS CONST_ACNT_NAME,
       DECODE(A.EXEC_AMT,0,0,TRUNC((B.EXEC_AMT/A.EXEC_AMT)*100,2))         AS EXEC_AMT,
       DECODE(A.CHG_AMT,0,0,TRUNC((B.CHG_AMT/A.CHG_AMT)*100,2))           AS CHG_AMT,
